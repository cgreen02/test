<div class="blog">
    <h1 class="blogTitle">{{ blog.title }}</h1>
    <div class="blogGrid">
        {% for article in blog.articles %}
            <a class="blogItem" href="{{ article.url }}">{{ article.title }}</a>
        {% endfor %}
    </div>
</div>


